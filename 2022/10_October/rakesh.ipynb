    "def height(n):\n",
    "    if n<=2:\n",
    "        return 0\n",
    "    else:\n",
    "        d=0\n",
    "        for i in range(2,n):\n",
    "            if n%i==0:\n",
    "                d=i\n",
    "                break\n",
    "        if d!=0:\n",
    "            return 1+height(n//i)\n",
    "        else:\n",
    "            return 0\n"
   ]
